    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/edit_cust_data"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ui_theme"
    tools:context=".show_data_vivid"
    tools:ignore="HardcodedText">


    <LinearLayout
        android:id="@+id/menulay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#00b0ff"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TooManyViews">

        <Button
            android:id="@+id/menu"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:background="@drawable/baseline_menu_24"
            android:elevation="8dp" />

        <TextView
            android:id="@+id/app_title_bar"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:paddingEnd="15dp"
            android:paddingStart="15dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:text="Emeraldsoff"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:padding="8dp"
            android:text="Mega Prospects"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            android:textStyle="bold" />

    </LinearLayout>
    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/gradient4flat2"
        android:scrollbars="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menulay"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="TooManyViews">

